            "required": true,
            "description": "Only filters by entities. Returns the specified entity from the model.",
            "schema": {
              "type": "string",
              "examples": [
                "Water"
              ]
            }
          }
        ],
        "responses": {
          "200": {
            "description": "A JSON array of objects",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "id": {
                        "type": "string",
                        "examples": [
                          "BDE2F1CF46B54EF58D33E20A230DA8D2"
                        ]
                      },
                      "name": {
                        "type": "string",
                        "examples": [
                          "Producto"
                        ]
                      }
